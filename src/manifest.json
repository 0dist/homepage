{
  "name": "homepage",
  "version": "0.4.0",
  "manifest_version": 3,
  "description": "Import bookmarks and images to organize and customize the new homepage",

  "permissions": [
    "bookmarks",
    "storage",
    "favicon"
  ],
  
  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "icons": {
    "32": "icons/logo/icon32.png",
    "128": "icons/logo/icon128.png"
  }
}